{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "vue template",
  "main": "app",
  "scripts": {
    "dist": "webpack --config webpack.prod.js",
    "build": "webpack --config webpack.dev.js",
    "watch": "vite serve",
    "devserver": "export NODE_ENV=development && node --inspect server/app",
    "server": "export NODE_ENV=production && nodemon server/app",
    "client": "export NODE_ENV=production && node client"
  },
  "keywords": [
    "vue",
    "vuex"
  ],
  "author": "Jeff Zhong",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-favicon": "^2.0.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^4.0.2",
    "mysql": "^2.18.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/compat": "^3.2.37",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.4",
    "pug": "^2.0.3",
    "sass": "^1.54.4",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1",
    "vite": "^3.0.7"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
